{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": ["@draven/ts-config/vue-app.json"],
  "include": ["src/**/*", "src/**/*.vue", "types/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
